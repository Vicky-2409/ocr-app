{
  "name": "ocr-app",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "build": "cd backend && npm ci && npm run build && cd ../frontend && npm ci && npm run build",
    "start": "cd backend && npm start",
    "dev": "concurrently \"npm run dev --workspace=frontend\" \"npm run dev --workspace=backend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
